<div class="readMore">
    <div class="readMoreTitle">
        <h3>LÃ¤s mer om {{ tags[1] }}</h3>
    </div>
    <div class="readMoreContent">
        {% for post in collections.all | exclude("posts") %}
            {% if post.data.tags == tags and post.data.title != title %}
                <a href="{{ post.url }}"><div class="readMorePost">
                    <img src="{{ post.data.image }}" alt="{{ post.data.alt }}">
                    <p>{{ post.data.title }}</p>
                </div></a>
            {% endif %}
        {% endfor %}
    </div>
</div>